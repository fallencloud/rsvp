{
  "name": "rsvp",
  "version": "1.0.0",
  "description": "A refactored Treehouse app",
  "main": "index.html",
  "scripts": {
    "watch": "concurrently \"npm run serve\" \"babel src -d dist --presets=es2015 -ws\"",
    "build": "rm -rf dist && babel src -d dist --presets=es2015",
    "serve": "http-server"
  },
