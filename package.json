    "JavaScript",
    "object-oriented",
    "JavaScript",
    "OOP",
    "front-end"
  ],
  "author": "Sharina V. Jones",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/fallencloud/rsvp/issues"
  },
  "homepage": "https://github.com/fallencloud/rsvp#readme",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "concurrently": "^4.1.0",
    "http-server": "^0.11.1"
  }
}
